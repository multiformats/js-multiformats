{
  "references": [
    {
      "path": "../src/"
    }
  ],
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": false,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictFunctionTypes": false,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "strictBindCallApply": true,
    "strict": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "target": "ES2020",
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "outDir": "../types",
    "skipLibCheck": true,
    "stripInternal": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "paths": {
      "multiformats": [
        "../src/index.js"
      ],
      "multiformats/*": [
        "../src/*.js"
      ]
    }
  },
  "include": [
    "test/",
    "."
  ],
  "exclude": [
    "ts-use/"
  ]
}