{
  "extends": "aegir/src/config/tsconfig.aegir.json",
  "compilerOptions": {
    "outDir": "dist/types",
    "emitDeclarationOnly": true,
    "paths": {
      "multiformats": [
        "./src/index.js"
      ],
      "multiformats/interface": [
        "./src/interface"
      ],
      "multiformats/hashes/interface": [
        "./src/hashes/interface"
      ],
      "multiformats/*": [
        "./src/*.js"
      ]
    }
  },
  "include": [
    "src",
    "test"
  ]
}
